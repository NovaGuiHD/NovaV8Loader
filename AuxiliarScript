btn=heal
function touch()
	local ms = game.Players.LocalPlayer:GetMouse()
	local c = game.CoreGui.NovaV7m.b2.Sample:Clone()
	c.Parent = btn
	local x, y = (ms.X - c.AbsolutePosition.X), (ms.Y - c.AbsolutePosition.Y)
	c.Position = UDim2.new(0, x, 0, y)
	local len, size = 0.35, nil
	if btn.AbsoluteSize.X >= btn.AbsoluteSize.Y then
		size = (btn.AbsoluteSize.X * 1.5)
	else
		size = (btn.AbsoluteSize.Y * 1.5)
	end
	c:TweenSizeAndPosition(UDim2.new(0, size, 0, size), UDim2.new(0.5, (-size / 2), 0.5, (-size / 2)), 'Out', 'Quad', len, true, nil)
	for i = 1, 10 do
		c.ImageTransparency = c.ImageTransparency + 0.05
		wait(len / 12)
	end
	c:Destroy()
end

btnclicktp.MouseButton1Click:Connect(function()
	btn=btnclicktp
	touch()
	local plr = game.Players.LocalPlayer
	local mouse = plr:GetMouse()
	local uis = game:GetService("UserInputService")
	
	local function clicktp(key)
		if key.KeyCode == Enum.KeyCode.LeftAlt then
			plr.Character.HumanoidRootPart.CFrame = mouse.Hit
		end
	end
	uis.InputBegan:Connect(clicktp)
end)

btnservertim.MouseButton1Click:Connect(function()
	btn=btnservertim
	touch()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/NovaGuiHD/NovaV8Loader/main/ServerTime"))()
end)

btnadmin.MouseButton1Click:Connect(function()
	btn=btnadmin
	touch()
	game.StarterGui:SetCore("SendNotification",{Title = "Activated";Text = "Auto kick you when staff joins";Duration = 4;})	
	target = { "MlgArcOfOz","dabiomaru","soulites","pyfrog","Solkami","Tomintown1","Salliokaru","LittleBrotherE","Timsuper","TheTrueCrab","YinTakoda","shxwnns","NLAUxz","LO_OP","YinTakoda","shxwnns","NLAUxz","LO_OP","Unerthodox","miami13","chartebar","YinTakoda","Wildi","EndebrosEAV","Tomintown1","thebigafricanluck","FlaireZol","ilovecashews","theuberashiin","Lemon_aaa","tofuthegreatest","Dragonwizarder","YinTakoda","RoxasTheFallen"} 
	
	game:GetService'Players'.PlayerAdded:Connect(function(player)
		for i,v in next, target do
			if player.Name == v then
				game:GetService'Players'.LocalPlayer:Kick("Staff Joined the game,kick for you safety")
			end
		end
	end)
end)
