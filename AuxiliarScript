local Fire=false
local player = game.Players.LocalPlayer
local mouse = player:GetMouse()
local uis = game:GetService("UserInputService")
local ap = Vector2.new(MaxS.AbsolutePosition.X, MaxS.AbsolutePosition.Y)
local as = Vector2.new(MaxS.AbsoluteSize.X, MaxS.AbsoluteSize.Y)
local x = mouse.X
local down = false
local apJ = Vector2.new(MaxJ.AbsolutePosition.X, MaxJ.AbsolutePosition.Y)
