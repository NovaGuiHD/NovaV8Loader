MaxS.MouseButton1Down:Connect(function()
	
	BarS.Size = UDim2.new(0, (mouse.X - ap.X), 1, 0)
	Fire = true
	down = true
	
end)

uis.InputEnded:Connect(function(input, gp)
	
	if input.UserInputType == Enum.UserInputType.MouseButton1 then
		
		down = false
		Fire= false
		
	end
	
end)

mouse.Move:Connect(function()
	
	if down == true then
		
		Fire= false
		Fire = true
		
		if mouse.X < ap.X then
			
			BarS.Size = UDim2.new(0, 0, 1, 0)
			
		elseif mouse.X > (ap.X + as.X) then
			
			BarS.Size = UDim2.new(0, as.X, 1, 0)
			
		else
			
			BarS.Size = UDim2.new(0, (mouse.X - ap.X), 1, 0)
			
		end
		
	end
	
end)



TextControlS.FocusLost:Connect(function()
	
	if typeof(tonumber(TextControlS.Text)) == "number" then
		
		local num = tonumber(TextControlS.Text)
		
		if num >= 0 and num <= 100 then
			
			BarS.Size = UDim2.new(0, ((num / 100) * as.X), 1, 0)
			
		else
			
			TextControlS.Text = "Min 0, max 100"
			
		end
		
	else
		
		TextControlS.Text = "Not valid"
		
	end
	
end)

MaxJ.MouseButton1Down:Connect(function()
	
	BarJ.Size = UDim2.new(0, (mouse.X - apJ.X), 1, 0)
	FireJ = true
	downJ = true
	
end)

uis.InputEnded:Connect(function(input, gp)
	
	if input.UserInputType == Enum.UserInputType.MouseButton1 then
		
		downJ = false
		FireJ= false
		
	end
	
end)

mouse.Move:Connect(function()
	
	if downJ == true then
		
		FireJ= false
		FireJ = true
		
		if mouse.X < apJ.X then
			
			BarJ.Size = UDim2.new(0, 0, 1, 0)
			
		elseif mouse.X > (apJ.X + asJ.X) then
			
			BarJ.Size = UDim2.new(0, asJ.X, 1, 0)
			
		else
			
			BarJ.Size = UDim2.new(0, (mouse.X - apJ.X), 1, 0)
			
		end
		
	end
	
end)



TextControlJ.FocusLost:Connect(function()
	
	if typeof(tonumber(TextControlJ.Text)) == "number" then
		
		local numJ = tonumber(TextControlJ.Text)
		
		if numJ >= 0 and numJ <= 100 then
			
			BarJ.Size = UDim2.new(0, ((numJ / 100) * asJ.X), 1, 0)
			
		else
			
			TextControlJ.Text = "Min 0, max 100"
			
		end
		
	else
		
		TextControlJ.Text = "Not valid"
		
	end
	
end)

while wait() do
	local r = (math.sin(workspace.DistributedGameTime/2)/2)+0.5
	local g = (math.sin(workspace.DistributedGameTime)/2)+0.5
	local b = (math.sin(workspace.DistributedGameTime*1.5)/2)+0.5
	local color = Color3.new(r, g, b)
	rainbow.BackgroundColor3 = color
	if Fire == true then
		
		local maxSize = as.X
		local size = BarS.Size.X.Offset
		local num = (size / maxSize)
		local num100 = 100 * (size / maxSize)
		
		TextControlS.Text = math.floor(num100)
		game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed = num100
		
	end
	if FireJ == true then
		
		local maxSizeJ = asJ.X
		local sizeJ = BarJ.Size.X.Offset
		local numJ = (sizeJ / maxSizeJ)
		local num100J = 100 * (sizeJ / maxSizeJ)
		
		TextControlJ.Text = math.floor(num100J)
		game:GetService("Players").LocalPlayer.Character.Humanoid.JumpPower = num100J
		
	end
end
