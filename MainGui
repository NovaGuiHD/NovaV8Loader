    local TweenService = game:GetService("TweenService")
    local tweenInfo2 = TweenInfo.new(1.5, Enum.EasingStyle.Quart, Enum.EasingDirection.InOut)
    local bobao = TweenService:Create(logo, tweenInfo2, {Rotation=360})
    local ms = game.Players.LocalPlayer:GetMouse()
    btn = config
    bobao:Play()
    bobao.Completed:Connect(function()
        logo.Rotation=0
        wait(5)
        bobao:Play()
    end)
loadstring(game:HttpGet("https://pastebin.com/raw/4VAVPL6e", true))() --halloween
    function touch()
        local c = Sample:Clone()
        c.Parent = btn
        local x, y = (ms.X - c.AbsolutePosition.X), (ms.Y - c.AbsolutePosition.Y)
        c.Position = UDim2.new(0, x, 0, y)
        local len, size = 0.35, nil
        if btn.AbsoluteSize.X >= btn.AbsoluteSize.Y then
            size = (btn.AbsoluteSize.X * 1.5)
        else
            size = (btn.AbsoluteSize.Y * 1.5)
        end
        c:TweenSizeAndPosition(UDim2.new(0, size, 0, size), UDim2.new(0.5, (-size / 2), 0.5, (-size / 2)), 'Out', 'Quad', len, true, nil)
        for i = 1, 10 do
            c.ImageTransparency = c.ImageTransparency + 0.05
            wait(len / 12)
        end
        c:Destroy()
    end
    --abrir e fechar
    btnLogoIn.MouseButton1Click:Connect(function()
        btn=btnLogoIn
        touch()	
        b2:TweenPosition(UDim2.new(0.001, 0,0.5, 0), 'In', 'Bounce', 1, true)
        wait(1)
        b3.Visible=false
    end)
    btnLogoBack.MouseButton1Click:Connect(function()
        btn=btnLogoBack
        touch()
        b2:TweenPosition(UDim2.new(-2, 0,0.5, 0), 'Out', 'Bounce', 1, true)
        b3.Visible=true
    end)
    --botoes do gui
    AntiAFK.MouseButton1Click:Connect(function()
        btn=AntiAFK
        touch()
        local afkbruh = game.CoreGui:FindFirstChild("AFKguiM")
        if afkbruh then
            print("alr")    
        else
            loadstring(game:HttpGet("https://pastebin.com/raw/ycpC45L9", true))()	
        end
    end)
    mainscript.MouseButton1Click:Connect(function()
        btn=mainscript
        touch()
        local main = game.CoreGui:FindFirstChild("NovaV8")
        if main then
            game.CoreGui.NovaV8.openedgui:TweenPosition(UDim2.new(0.517, 0, 0.547, 0))
        else
            loadstring(game:HttpGet("https://pastebin.com/raw/GP44TzbN", true))() 	
        end
    end)
    credits.MouseButton1Click:Connect(function()
        btn=credits
        touch()
        local credi = game.CoreGui:FindFirstChild("creditsgui")
        if credi then
            print("alr")    
        else
            loadstring(game:HttpGet("https://pastebin.com/raw/fqTcg89U", true))()	
        end
    end)
    sosscript.MouseButton1Click:Connect(function()
        btn=sosscript
        touch()
        local sos = game.CoreGui:FindFirstChild("helpgui")
        if sos then
            print("alr")    
        else
            loadstring(game:HttpGet("https://pastebin.com/raw/iKaP5YcB", true))()	
        end
    end)
    config.MouseButton1Click:Connect(function()
        btn=config
        touch()
        local con = game.CoreGui:FindFirstChild("optionsgui")
        if con then
            game.CoreGui.optionsgui.back.Visible=true    
        else
            loadstring(game:HttpGet("https://raw.githubusercontent.com/NovaGuiHD/NovaV8Loader/main/Configs"))()	
        end
    end)
	somescript.MouseButton1Click:Connect(function()
        btn=somescript
        touch()
        local con = game.CoreGui:FindFirstChild("AuxiliarScript")
        if con then
            game.CoreGui.AuxiliarScript.back.Visible=true    
        else
            loadstring(game:HttpGet("https://pastebin.com/raw/vhXwRcaM", true))()	
        end
    end)
    while wait() do
        color = Color3.new((math.sin(workspace.DistributedGameTime/2)/2)+0.5, (math.sin(workspace.DistributedGameTime)/2)+0.5, (math.sin(workspace.DistributedGameTime*1.5)/2)+0.5)
        barmaingui.BackgroundColor3 = color
        astand.Text = game.Players.LocalPlayer.Stand.Value
        bfuse.Text = game.Players.LocalPlayer.FusedStand.Value
        cbless.Text = game.Players.LocalPlayer.Blessing.Value
        eworth.Text = "Worth: " .. game.Players.LocalPlayer.Worth.Value
        dtrait.Text = game.Players.LocalPlayer.Trait.Value
    end
