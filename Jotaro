Title = "Jotaro Quest";
Text ="Kill 5 Evil Star Platinum users";
Duration = 10;
})
if game.PlaceId==2731332132 then
local espu = workspace:WaitForChild("Evil Star Platinum User")
if espu then
local plr = game.Players.LocalPlayer.Character

while wait() do
for _,v in pairs(workspace:GetChildren()) do
if v:FindFirstChild("Humanoid") and v.Name == "Evil Star Platinum User" then
game:GetService("ReplicatedStorage").Logic.hitbox:InvokeServer(0,plr["Right Arm"],CFrame.new(0,0,0),math.huge,plr.Torso.voiceline,v:FindFirstChild("Humanoid"))
end
end
end
end

else
	game:GetService("TeleportService"):Teleport(2731332132, LocalPlayer)
end
end
