})
if game.PlaceId==2295122555 and game.Players.LocalPlayer.Money.Value <=15000 then
	for i,v in next, debug.getregistry() do
	if type(v) == "function" then
		for a,b in next, debug.getupvalues(v) do
			if string.find(a, "cooled") or string.find(a, "cooled2") or string.find(a, "cooled3") or string.find(a, "cooled4") or string.find(a, "cooled5") or string.find(a, "cooledz") then
				debug.setupvalue(v, a, true)
			end
		end
	end
	 end
	
local function Kills()
while game.Players.LocalPlayer.Money.Value<=15000 do 
 for i,v in pairs(game.Workspace:GetChildren()) do
   if v.Name == "Giant Dummy" or v.Name == "Metal Dummy" or v.Name == "Wood Dummy" or v.Name == "Ice Dummy" or v.Name == "Rock Dummy" then
	  local A_1 = CFrame.new(nil, nil, nil)
      local A_2 = 1000000000000
      local A_3 = v.Humanoid
      local A_4 = game.Players.LocalPlayer.Character.Torso.voiceline
      local A_5 = false
      local Event = game:GetService("ReplicatedStorage").Specials.healevent
            Event:FireServer(A_1, A_2, A_3, A_4, A_5)
	game.ReplicatedStorage.Logic.hitbox:InvokeServer(99, game.Players.LocalPlayer.Character.Head, CFrame.new(nil, nil, nil), 1000000000000, game.Players.LocalPlayer.Character.Torso.voiceline, v.Humanoid, false, false, false)

    if game.Players.LocalPlayer.Money.Value>=15000 then break end

   end
 end
end
end
for i = 1, 130 do
  spawn(Kills)
end
elseif game.PlaceId==2731332132 then
	game:GetService("TeleportService"):Teleport(2295122555, LocalPlayer)
elseif game.Players.LocalPlayer.Money.Value >=15000 then
	game.ReplicatedStorage.Logic.giveitem:FireServer("VampireMask")
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1908,875,-1301) 
end	
end

	
