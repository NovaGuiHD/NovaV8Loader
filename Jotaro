local checkjot = game.Players.LocalPlayer.currentjotaro.Value
local Enabled=false
if checkjot == 0 then
game.StarterGui:SetCore("SendNotification", {
Title = "Jotaro Quest";
Text = "Talk to Jotaro"; 
Duration = 10;
})
if game.PlaceId==2295122555 then
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1908,875,-1301)
else
	game:GetService("TeleportService"):Teleport(2295122555, LocalPlayer)
end
end
if checkjot == 1 then
